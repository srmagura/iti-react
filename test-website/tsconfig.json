{
  "compilerOptions": {
    "target": "es5",
    "sourceMap": true,
    "jsx": "react",
    "baseUrl": "./src",
    "allowJs": false,
    "moduleResolution": "node",
    "module": "esnext",
    "strict": true,
    "types": [ "webpack-env" ],
    "lib": [ "es2017", "dom" ],
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "node_modules",
    "obj"
  ]
}
