    <PackageReference Include="Microsoft.VisualStudio.Web.CodeGeneration.Design" Version="2.0.3" />
  </ItemGroup>

  <ItemGroup>
    <TypeScriptCompile Include="Client\Scripts\Api.ts" />
    <TypeScriptCompile Include="Client\Scripts\Components\ProcessError.ts" />
    <TypeScriptCompile Include="Client\Scripts\Components\Routing\RouteProps.tsx" />
