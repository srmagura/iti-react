<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net47</TargetFramework>
    <TypeScriptToolsVersion>Latest</TypeScriptToolsVersion>
    <TypeScriptCompileBlocked>True</TypeScriptCompileBlocked>
    <Configurations>Debug;Release;Staging</Configurations>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <PlatformTarget>x86</PlatformTarget>
  </PropertyGroup>

  <ItemGroup>
    <Content Remove="Client\Index.cshtml" />
    <Content Remove="package-lock.json" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Client\s\Util\ErrorUtil.ts" />
    <None Remove="Client\s\Util\ValidationLib\Readme.md" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Client\Scripts\Components\Footer.tsx" />
    <Content Include="Client\Scripts\Components\Header.tsx" />
    <Content Include="Client\Scripts\Components\Layout.tsx" />
    <Content Include="Client\s\Models\ErrorDto.ts" />
    <Content Include="Client\s\Models\ErrorIndexViewModel.ts" />
    <Content Include="Client\s\Models\ExampleUserDto.ts" />
    <Content Include="Client\s\Models\HomeIndexViewModel.ts" />
    <Content Include="Client\s\Models\IanaTimeZone.ts" />
    <Content Include="Client\s\Models\index.ts" />
    <Content Include="Client\s\Models\Models.tst" />
    <Content Include="Client\s\Models\ReactViewModel.ts" />
    <Content Include="Client\s\Models\ViewModel.ts" />
    <Content Include="Client\s\Pages\Error\Index.tsx" />
    <Content Include="Client\s\Pages\Example\Ajax.tsx" />
    <Content Include="Client\s\Pages\Example\DateTime.tsx" />
    <Content Include="Client\s\Pages\Example\Form.tsx" />
    <Content Include="Client\s\Pages\Home\Index.tsx" />
    <Content Include="Client\s\Url.ts" />
    <Content Include="Client\s\Util\AjaxUtil.ts" />
    <Content Include="Client\s\Util\BrowserUtil.ts" />
    <Content Include="Client\s\Util\DateTimeUtil.ts" />
    <Content Include="Client\s\Util\FormUtil\AntiForgeryToken.tsx" />
    <Content Include="Client\s\Util\FormUtil\Functions.ts" />
    <Content Include="Client\s\Util\FormUtil\index.ts" />
    <Content Include="Client\s\Util\PaginationHelpers.ts" />
    <Content Include="Client\s\Util\Plumbing\ErrorUtil.ts" />
    <Content Include="Client\s\Util\Plumbing\Log.ts" />
    <Content Include="Client\s\Util\UrlUtil.ts" />
    <Content Include="Client\s\Util\ValidationLib\FieldValidity.ts" />
    <Content Include="Client\s\Util\ValidationLib\index.ts" />
    <Content Include="Client\s\Util\ValidationLib\InputWithFeedback.tsx" />
    <Content Include="Client\s\Util\ValidationLib\Readme.md" />
    <Content Include="Client\s\Util\ValidationLib\ReadOnlyInput.tsx" />
    <Content Include="Client\s\Util\ValidationLib\ValidatedInput.tsx" />
    <Content Include="Client\s\Util\ValidationLib\ValidatorCore.ts" />
    <Content Include="Client\s\Util\ValidationLib\Validators.tsx" />
    <Content Include="webpack.config.js" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="wwwroot\dist\" />
    <Folder Include="wwwroot\NewFolder1\" />
    <Folder Include="wwwroot\NewFolder\" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore" Version="2.0.2" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc" Version="2.0.3" />
    <PackageReference Include="Microsoft.AspNetCore.SpaServices" Version="2.0.3" />
    <PackageReference Include="Microsoft.AspNetCore.StaticFiles" Version="2.0.2" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="2.0.2" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="2.0.2" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Tools" Version="2.0.2" />
    <PackageReference Include="Microsoft.Extensions.Configuration" Version="2.0.1" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Binder" Version="2.0.1" />
    <PackageReference Include="Microsoft.VisualStudio.Web.BrowserLink" Version="2.0.2" />
    <PackageReference Include="Microsoft.VisualStudio.Web.CodeGeneration.Design" Version="2.0.3" />
  </ItemGroup>

  <ItemGroup>
    <TypeScriptCompile Include="Client\s\Util\Plumbing\ErrorUtil.ts" />
    <TypeScriptCompile Include="Client\s\Util\ValidationLib\Readme.md" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Client\s\Models\ErrorDto.ts">
      <CustomToolNamespace>Dto\ErrorDto.cs</CustomToolNamespace>
    </None>
    <None Update="Client\s\Models\ErrorIndexViewModel.ts">
      <CustomToolNamespace>ViewModels\Error\ErrorIndexViewModel.cs</CustomToolNamespace>
    </None>
    <None Update="Client\s\Models\ExampleUserDto.ts">
      <CustomToolNamespace>Dto\ExampleUserDto.cs</CustomToolNamespace>
    </None>
    <None Update="Client\s\Models\HomeIndexViewModel.ts">
      <CustomToolNamespace>ViewModels\Home\HomeIndexViewModel.cs</CustomToolNamespace>
    </None>
    <None Update="Client\s\Models\IanaTimeZone.ts">
      <CustomToolNamespace>Dto\IanaTimeZone.cs</CustomToolNamespace>
    </None>
    <None Update="Client\s\Models\ReactViewModel.ts">
      <CustomToolNamespace>ViewModels\Base\ReactViewModel.cs</CustomToolNamespace>
    </None>
    <None Update="Client\s\Models\ViewModel.ts">
      <CustomToolNamespace>ViewModels\Base\ViewModel.cs</CustomToolNamespace>
    </None>
  </ItemGroup>

  <Target Name="PostBuild" AfterTargets="PostBuildEvent">
    <Exec Command="powershell.exe -ExecutionPolicy Bypass -NoProfile -NonInteractive -File &quot;$(ProjectDir)postbuild.ps1&quot; -projectConfiguration $(ConfigurationName)" />
  </Target>

</Project>
