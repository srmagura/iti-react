    <None Remove="wwwroot\NewFolder\**" />
  </ItemGroup>

  <ItemGroup>
    <Content Remove="Client\Index.cshtml" />
    <Content Remove="package-lock.json" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Client\Scripts\Api.ts" />
    <None Remove="Client\Scripts\Components\ProcessError.ts" />
    <None Remove="Client\Scripts\Components\RouteProps.tsx" />
    <None Remove="Client\Scripts\Library.d.ts" />
    <None Remove="Client\Scripts\Pages\Home\Components.tsx" />
    <None Remove="Client\Scripts\Pages\Home\Inputs.tsx" />
    <None Remove="Client\Scripts\Pages\Home\UrlParam.tsx" />
    <None Remove="Client\Scripts\Routes.tsx" />
    <None Remove="Client\Scripts\Util\AsyncLink.tsx" />
    <None Remove="Client\Scripts\Util\ITIReact\Changelog.md" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Client\Scripts\Components\Footer.tsx" />
    <Content Include="Client\Scripts\Components\Header.tsx" />
    <Content Include="Client\Scripts\Components\Layout.tsx" />
    <Content Include="Client\Scripts\Models\Models.tst" />
    <Content Include="Client\Scripts\Pages\Home\UrlParam.tsx" />
    <Content Include="Client\Scripts\Pages\Home\Inputs.tsx" />
    <Content Include="Client\Scripts\Pages\Home\Components.tsx" />
    <Content Include="Client\Scripts\Pages\Home\Form.tsx" />
    <Content Include="Client\Scripts\Util\BrowserUtil.ts" />
